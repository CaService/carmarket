{
  "version": 1,
  "include": ["/*"],
  "exclude": ["/static/*", "/api/*"],
  "rules": [
    {
      "pattern": "/carmarket/static/pdf/*",
      "cache": {
        "browser": {
          "maxAgeSeconds": 3600
        },
        "edge": {
          "maxAgeSeconds": 86400
        }
      }
    },
    {
      "pattern": "/carmarket/**",
      "serve": "/carmarket/index.html",
      "status": 200
    }
  ]
}
