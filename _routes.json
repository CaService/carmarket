{
  "version": 1,
  "include": ["/*"],
  "exclude": [
    "/repositories/carmarket/server/api/*",
    "/repositories/carmarket/static/*"
  ],
  "rules": [
    {
      "pattern": "/repositories/carmarket/*",
      "serve": "/repositories/carmarket/index.html",
      "status": 200,
      "headers": {
        "Cache-Control": "no-cache",
        "Content-Type": "text/html; charset=utf-8",
        "X-Frame-Options": "DENY"
      }
    }
  ]
}
